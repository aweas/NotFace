<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wwydm.notfaceandroid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MANAGE_DOCUMENTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".faceDetectionActivity"
            android:label="@string/title_activity_face_detection"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".faceDetectionServer"
            android:label="@string/title_activity_face_detection_server"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".photosOverview"
            android:label="@string/title_activity_photos_overview" />
        <activity
            android:name=".photosDetails"
            android:label="@string/title_activity_photos_details"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>